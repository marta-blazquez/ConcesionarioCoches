<table class="table">
    <thead class="thead-light">
      <tr>
        <th scope="col">Name</th>
        <th scope="col">Surname</th>
        <th scope="col">Telephone_number</th>
        <th scope="col">email</th>
      </tr>
    </thead>
    <% result.forEach(student =>{ %>
    <tbody>
      <tr>
        <td><%= student.name %></td>
        <td><%= student.surname %></td>
        <td><%= student.telephone_number %></td>
        <td><%= student.email %></td>
      </tr> 
      <% }) %>
    </tbody>
  </table>

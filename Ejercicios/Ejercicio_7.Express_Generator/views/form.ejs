        <div class="col-12">
            <h1>Directorio de empresas</h1>
            <form action="form/recogerDatos" method="POST">
                <div class="form-group">
                  <label for="exampleInputPassword1">Nombre de la web</label>
                  <input type="text" class="form-control" name="nombreDeLaEmpresa">
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword1">Dominio</label>
                    <input type="text" class="form-control" name="dominio">
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword1">Colores corporativos</label>
                    <input type="text" class="form-control" name="coloresCorporativos">
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword1">Telefono</label>
                    <input type="number" class="form-control" name="telefono">
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword1">Direccion</label>
                    <input type="text" class="form-control" name="direccion">
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword1">CEO</label>

                <div class="form-group">
                    <label for="exampleInputPassword1">CTO</label>
                    <input type="text" class="form-control" name="cto">
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword1">Front developer</label>
                    <input type="text" class="form-control" name="frontDeveloper">
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword1">ux/ui</label>
                    <input type="text" class="form-control" name="uxUi">
                </div>
        
                <button type="submit" class="btn btn-primary">Enviar</button>
              </form>
        </div>
    </div>
</div>
